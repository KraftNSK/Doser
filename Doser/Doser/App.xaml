<Application x:Class="Doser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewmodel="clr-namespace:Doser.ViewModels"
             xmlns:view="clr-namespace:Doser.Views"
             xmlns:local="clr-namespace:Doser">
    
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodel:ConfigActiveBunkersWindowViewModel}">
            <view:ConfigActiveBunkersWindow HorizontalAlignment="Stretch" />
        </DataTemplate>
        <ResourceDictionary x:Key="dc">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>  
    </Application.Resources>
</Application>
